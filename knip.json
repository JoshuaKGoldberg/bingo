{
	"$schema": "https://unpkg.com/knip@latest/schema.json",
	"ignoreExportsUsedInFile": { "interface": true, "type": true },
	"workspaces": {
		"packages/create": {
			"project": ["src/**/*.ts!"],
			"entry": ["src/index.ts!"]
		},
		"packages/create-testers": {
			"project": ["src/**/*.ts!"],
			"entry": ["src/index.ts!"]
		},
		"packages/site": {
			"ignore": ["sharp"],
			"entry": ["astro.config.ts"]
		}
	}
}
